LEXICON Indefinite !!= @CODE@ \\
awiyak+Pron+Indef+A+Sg:awiyak # "someone" ; !!= @CODE@
awiyak+Pron+Indef+A+Pl:awiyakak # "some people" ;!!= @CODE@

LEXICON Demonstrative !!= @CODE@   \\
!! ANIMATE \\
awa+Pron+Dem+Prox+A+Sg:awa # "this" ; !!= @CODE@
ôko+Pron+Dem+Prox+A+Pl:ôko # "these" ; !!= @CODE@
ôho+Pron+Dem+Prox+A+Obv:ôho # "this/these" ; !!= @CODE@

ana+Pron+Dem+Med+A+Sg:ana # "that" ;
aniki+Pron+Dem+Med+A+Pl:aniki # "those" ;
anikik+Pron+Dem+Med+A+Pl:anikik # "those" ;
anihi+Pron+Dem+Med+A+Pl:anihi # "that/those" ;

nâha+Pron+Dem+Dist+A+Sg:nâha # "that yonder" ;
nîki+Pron+Dem+Dist+A+Pl:nîki # "those yonder" ;
nîhi+Pron+Dem+Dist+A+Pl:nîhi # "this/those yonder" ;

!! INANIMATE \\

ôma+Pron+Dem+Prox+I+Sg:ôma # "this" ; !!= @CODE@
ôho+Pron+Dem+Prox+I+Pl:ôho # "these" ; !!= @CODE@
ômîthiw+Pron+Dem+Prox+I+Obv:ômîthiw # "this/these" ; !!= @CODE@

anima+Pron+Dem+Med+I+Sg:anima # "that" ;
anihi+Pron+Dem+Med+I+Pl:anihi # "those" ;
animîthiw+Pron+Dem+Med+I+Obv:animîthiw # "that/those" ;

nîma+Pron+Dem+Dist+I+Sg:nîma # "that yonder" ;
nîhi+Pron+Dem+Dist+I+Pl:nîhi # "those yonder" ;
nîmîthiw+Pron+Dem+Dist+I+Obv:nîmîthiw # "that/those yonder" ;

! Let us say a pronoun ôma without a noun is a definite, not a demonstrative
ôma+Pron+Def+Prox+I+Sg:ôma # "this one" ; !!= @CODE@
ôho+Pron+Def+Prox+I+Pl:ôho # "these one" ; !!= @CODE@
ômîthiw+Pron+Def+Prox+I+Obv:ômîthiw # "this/these one(s)" ; !!= @CODE@

anima+Pron+Def+Med+I+Sg:anima # "that one" ;
anihi+Pron+Def+Med+I+Pl:anihi # "those one" ;
animîthiw+Pron+Def+Med+I+Obv:animîthiw # "that/those one(s)" ;

nîma+Pron+Def+Dist+I+Sg:nîma # "that yonder one" ;
nîhi+Pron+Def+Dist+I+Pl:nîhi # "those yonder one" ;
nîmîthiw+Pron+Def+Dist+I+Obv:nîmîthiw # "that/those yonder one(s)" ;
