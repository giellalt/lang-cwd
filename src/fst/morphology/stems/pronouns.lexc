!! ## Plains Cree pronouns

!! There are more pronoums to be added here.

LEXICON Pronoun !!= @CODE@

 Personal ;
 Interrogative ;
 Indefinite ;
 Demonstrative ;

LEXICON Personal !!= @CODE@ \\
nîtha+Pron+Pers+1Sg:nîtha # ; !!= @CODE@
kîtha+Pron+Pers+2Sg:kîtha # ; !!= @CODE@
wîtha+Pron+Pers+3Sg:wîtha # ;
nîthanân+Pron+Pers+1Pl:nîthanân # ;
kîthânaw+Pron+Pers+12Pl:kîthânaw # ;
kîthawâw+Pron+Pers+2Pl:kîthawâw # ;
wîthawâw+Pron+Pers+3Pl: wîthawâw # ;

nîsta+Pron+Pers+1Sg+Incl:nîsta # ;
kîsta+Pron+Pers+2Sg+Incl:kîsta # ;
wîsta+Pron+Pers+3Sg+Incl:wîsta # ;
nîstanân+Pron+Pers+1Pl+Incl:nîstanân # ;
kîstanaw+Pron+Pers+12Pl+Incl:kîstanaw # ;
kîstawâw+Pron+Pers+2Pl+Incl:kîstawâw # ;
wîstawâw+Pron+Pers+3Pl+Incl:wîstawâw # ;


LEXICON Interrogative  !!= @CODE@ \\
awîna+Pron+Interr+A+Sg:awîna # "who,whose" ; !!= @CODE@
awînak+Pron+Interr+A+Sg:awînak # "who,whose" ; !!= @CODE@
awîna+Pron+Interr+A+Pl:awîna # "who,whose" ; !!= @CODE@
awînak+Pron+Interr+A+Pl:awînak # "who,whose" ; !!= @CODE@
awîniki+Pron+Interr+A+Pl+Var/East:awîniki # "who" ; !!= @CODE@
awînikik+Pron+Interr+A+Pl+Var/East:awînikik # "who" ; !!= @CODE@
awîna+Pron+Interr+A+Obv:awîna # "who,whose" ; !!= @CODE@
awînithiwa+Pron+Interr+A+Obv+Var/East:awînithiwa # "who" ; !!= @CODE@
awînaka+Pron+Interr+A+Obv+Var/East:awînaka # "who" ; !!= @CODE@

kîkway+Pron+Interr+I+Sg:kîkway # "what" ;
kîkwan+Pron+Interr+I+Sg+Var/East:kîkwan # "what" ;
kîkwaya+Pron+Interr+I+Pl:kîkwaya # "what" ;
kîkwâtha+Pron+Interr+I+Pl:kîkwâtha # "what" ;
kîkway+Pron+Interr+I+Pl+Var/East:kîkway # "what" ;
kîkwan+Pron+Interr+I+Pl+Var/East:kîkwan # "what" ;
kîkwâthiw+Pron+Interr+I+Obv:kîkwâthiw # "what" ;
kîkwan+Pron+Interr+I+Obv+Var/East:kîkwan # "what" ;

! Interrogative demonstratives
tâna+Pron+Interr+Dem+A+Sg:tâna # "which one" ;
tânitowihk+Pron+Interr+Dem+A+Sg:tânitowihk # "which one" ;
tânîtwâ+Pron+Interr+Dem+A+Sg:tânîtwâ # "which one" ;
tânihkâ+Pron+Interr+Dem+A+Pl:tânihkâ # "which ones" ;
tâniwîhi+Pron+Interr+Dem+A+Obv:tâniwîhi # "which one(s)" ;
tānima+Pron+Interr+Dem+I+Sg:tānima # "which one" ;
tāni+Pron+Interr+Dem+I+Sg:tāni # "which one" ;
tānitowihkān+Pron+Interr+Dem+I+Sg:tānitowihkān # "which one" ;
tânîhi+Pron+Interr+Dem+I+Pl:tânîhi # "which ones" ;
tānitowihkān+Pron+Interr+Dem+I+Pl:tānitowihkān # "which one" ;

! Locative interrogative demonstratives
tâniwâ+Pron+Interr+Dem+Loc+A+Sg:tâniwâ # "where" ; ! Wolvengrey
tâniwîhkâ+Pron+Interr+Dem+Loc+A+Pl:tâniwîhkâ # "where" ;
tânîhkâ+Pron+Interr+Dem+Loc+A+Pl:tânîhkâ # "where" ;
tâniwîhâ+Pron+Interr+Dem+Loc+A+Obv:tâniwîhâ # "where" ;
tânîhâ+Pron+Interr+Dem+Loc+A+Obv:tânîhâ # "where" ;
tâniwî+Pron+Interr+Dem+Loc+I+Sg:tâniwî # "where" ;
tâniwîhâ+Pron+Interr+Dem+Loc+I+Pl:tâniwîhâ # "where" ;
tânîhâ+Pron+Interr+Dem+Loc+I+Pl:tânîhâ # "where" ;

